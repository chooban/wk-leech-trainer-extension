{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "semicolon": [
      true,
      "never"
    ],
    "interface-name": [
      false
    ],
    "object-literal-sort-keys": [
      false
    ]
  },
  "rulesDirectory": []
}

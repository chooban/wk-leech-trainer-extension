{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "noImplicitAny": false,
    "removeComments": true,
    "esModuleInterop": true,
    "sourceMap": false,
    "jsx": "react",
    "jsxFactory": "h",
    "types": ["chrome"],
    "rootDir": "./src",
    "outDir": "./dist"
  },
  "include": ["./src/**/*.tsx", "./src/**/*.ts", "../wkjs/**/*.ts"],
  "exclude": ["node_modules"]
}
